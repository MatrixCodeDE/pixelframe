{
  "backup": {
    "enabled": false,
    "interval": 60,
    "directory": "Backup/backup_files"
  },
  "connection": {
    "host": "0.0.0.0",
    "ports": {
      "socket": 1234,
      "api": 8443
    },
    "timeout": 30
  },
  "game": {
    "pps": 10,
    "godmode": {
      "enabled": true,
      "pps": 1000000
    }
  },
  "general": {
    "name": "PixelFrame"
  },
  "frontend": {
    "display": {
      "enabled": false,
      "fps": 10
    },
    "api": {
      "enabled": true,
      "enable_admin": true
    },
    "sockets": {
      "enabled": true,
      "enable_admin": false
    },
    "web": {
      "force_reload": false
    }
  },
  "timelapse": {
    "enabled": false,
    "interval": 60,
    "directory": "Backup/timelapse_files"
  },
  "visuals": {
    "size": {
      "width": 1920,
      "height": 1080
    },
    "statsbar": {
      "enabled": false,
      "size": 30
    }
  }
}